<div id="carouselExampleCaptions" class="carousel slide">
  <div class="carousel-indicators">
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
      aria-current="true" aria-label="Slide 1"></button>
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
  </div>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="/images/Group-project-image-main.jpg" class="d-block w-100"
        alt="A recently married couple looking at eachother">
      <div class="carousel-caption d-none d-md-block">
        <h1 class="tag">Welcome to Wedding Bells</h1>
        <h4 class="tag">Are you ready to prepare for your future?</h4>
      </div>
    </div>
    <div class="carousel-item">
      <img src="/images/flowerPicture.jpg" class="d-block w-100" alt="Picture of flowers">
      <div class="carousel-caption d-none d-md-block">
        <h1 class="tag">Pick from amazing photographers and florist where your special moment is!</h1>
        <h4 class="tag">We care about you and every deatil</h4>
      </div>
    </div>
    <div class="carousel-item">
      <img src="/images/jordansRings.jpg" class="d-block w-100" alt="Recently married couple kissing">
      <div class="carousel-caption d-none d-md-block">
        <h1 class="tag">We have amazing venues no matter where you are located</h1>
        <h3 class="tag">Where you are, we are. Planning made easy</h3>
      </div>
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>

<script src="./js/homepage.js"></script>

{{#each weddings}}
{{log this}}
{{#if this.married}}
{{else}}
<h1>{{this.name}}</h1>
{{/if}}
{{/each}}
</div>
<nav class="navBar2">
  <a href="/">Home</a> |
  <a href="/profile">"Login or Sign up"</a> |
  {{#if logged_in}}
  <button class="no-button" id="logout">Logout</button>
  {{else}}
  <a href="/login">Login</a>
  {{/if}}
</nav>